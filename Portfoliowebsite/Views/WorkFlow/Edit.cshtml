    <input asp-for="User_Id" hidden/>
    <span asp-validation-for="WorkFlowList" class="text-danger"></span><br />
    <label>Enter Your Work:</label>
    <input type="text" asp-for="WorkFlowList" /><br /><br />

    <input type="submit" value="Submit" />
</form>